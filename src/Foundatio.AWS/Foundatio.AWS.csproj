  <Import Project="..\..\build\common.props" />
  <PropertyGroup>
    <PackageTags>$(PackageTags);Amazon;AWS;S3</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.CloudWatch" Version="3.7.103.4" />
